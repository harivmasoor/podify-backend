{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/spotify/token",
        "dest": "./server.js",
        "methods": ["POST"]
      },
      {
        "src": "/",
        "dest": "./server.js",
        "methods": ["GET"]
      }
    ]
  }
  
  